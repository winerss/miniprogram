.container {
  position: relative;
  height: 360px;
  width: 320px;
}
.img {
   position: absolute;
   top: 0;
   width: 320px;
   height: 360px;
   left: 0;
   /* transform: translateX(-160px); */
   overflow: hidden;
}
.footer {
  position: absolute;
  width: 100%;
  height: 110rpx;
  color: #fff;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.print{
  position: absolute;
  z-index: 999;
  height: 50rpx;
  width: 50rpx;
  border-radius: 50%;
  bottom: 0rpx;
  right: 0rpx;
  line-height: 50rpx;
  font-size: 18rpx;
  color: #ccc;
  background: rgba(0, 0, 0, 0.4);
}
.background {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  z-index: -1;
}